class Solution:
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        solu_index = []
        
        length = len(nums)
        for i in range(length-1):
            delta = target - nums[i]
            if delta in nums and delta != nums[i]:
                solu_index.append(i)
                solu_index.append(nums.index(delta))
                return solu_index
                break
            else:
                pass
        if solu_index == []:
            solu_index = 'No two sum solution'
        else:
            pass
        return solu_index

# https://leetcode.com/problems/two-sum/

"""
Something learned:
1. 注意判断循环的里面和外面。
2. nums.index(delta)可以取到列表中某数的index。
3. 注意 break 的使用。
"""
